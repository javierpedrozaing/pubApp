         visibleItems: 6,
         animationSpeed: 700,
         autoPlay: true,
         autoPlaySpeed: 2000,            
         pauseOnHover: true,
         enableResponsiveBreakpoints: true,
         responsiveBreakpoints: { 
             portrait: { 
                 changePoint:480,
                 visibleItems: 3
             }, 
             landscape: { 
                 changePoint:640,
                 visibleItems: 4
             },
             tablet: { 
                 changePoint:768,
                 visibleItems: 5
            }
        }
    });

    $(".submenu > li").click(function(e){
            var a = e.target.id;
            //desactivamos seccion y activamos elemento de menu
            $(".submenu li.active").removeClass("active");
            $(".submenu #"+a).addClass("active");
        });
    });
  </script>
